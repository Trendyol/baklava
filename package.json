{
  "name": "@trendyol-js/grace",
  "version": "1.0.39",
  "description": "Trendyol Grace - Component Design",
  "main": "dist/grace.js",
  "module": "dist/grace.js",
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Trendyol Tech",
    "url": "https://github.com/Trendyol"
  },
  "license": "MIT",
  "scripts": {
    "prestart": "npm run prep",
    "prepare": "husky install",
    "start": "npm run storybook:dev:watch",
    "prep": "npm-run-all -s prep:*",
    "prep:component-list": "wca analyze 'src/components/**/!(*.test).ts' --features --format json --outFile ./src/component-list.json",
    "prep:custom-elements": "wca analyze 'src/components/**/!(*.test).ts' --format json --outFile ./src/custom-elements.json",
    "prebuild": "npm run prep",
    "build": "node scripts/build.js",
    "build:dev": "node scripts/build.js",
    "serve": "node scripts/build.js --serve",
    "storybook:axe:prod": "npm run storybook:prod && npm run axe-storybook --build-dir=storybook-static/",
    "storybook:axe:dev": "npm-run-all -s storybook:axe:dev:chromium storybook:axe:dev:firefox",
    "storybook:axe:dev:chromium": "npm run axe-storybook --storybook-address=\"http://localhost:1994\" --browser chromium",
    "storybook:axe:dev:firefox": "npm run axe-storybook --storybook-address=\"http://localhost:1994\" --browser firefox",
    "storybook:axe:dev:webkit": "npm run axe-storybook --storybook-address=\"http://localhost:1994\" --browser webkit",
    "storybook:dev": "start-storybook -p 1994",
    "storybook.prod": "npm run storybook:prod",
    "storybook:prod": "npm run prep && npm run storybook:wait && NODE_ENV=production build-storybook -o storybook-static",
    "lint": "npm-run-all -s lint:*",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check src/",
    "lint:style": "stylelint --fix src/components/**/*.css",
    "format": "npm-run-all -s format:*",
    "format:code": "eslint --fix ./src && prettier --write ./src",
    "fix": "npm run fmt && npm run lint",
    "ci": "rm -rf node_modules && npm install --frozen-lockfile",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "clean:generated": "rimraf src/custom-elements.json && rimraf src/component-list.json",
    "clean:modules": "rimraf node_modules",
    "reset": "npm run clean && npm install",
    "reboot": "npm run clean && npm install && npm start",
    "test": "web-test-runner --coverage",
    "test:component": "npm run test -- --group",
    "test:watch": "web-test-runner --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "commit": "commit"
  },
  "keywords": [
    "web-components",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "@fontsource/rubik": "^4.5.9",
    "@types/estree": "0.0.47",
    "lit": "^2.2.3"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/prompt-cli": "^16.2.4",
    "@open-wc/testing": "^3.0.0-next.2",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-storysource": "^6.4.22",
    "@storybook/web-components": "^6.4.22",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@web/dev-server-esbuild": "^0.3.0",
    "@web/dev-server-import-maps": "^0.0.6",
    "@web/dev-server-rollup": "^0.3.17",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-playwright": "^0.8.6",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "chromatic": "^6.5.4",
    "del": "^6.1.0",
    "esbuild": "^0.14.39",
    "esbuild-plugin-lit-css": "^2.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "globby": "^13.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-lit": "^0.4.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-lit-css": "^4.0.0",
    "storybook": "^6.4.19",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.6.3",
    "wait-on": "^5.3.0",
    "web-component-analyzer": "^1.0.3"
  },
  "lint-staged": {
    "**/*.{js,,ts,}": [
      "bash -c tsc --noEmit",
      "npx prettier --write",
      "npx eslint --fix"
    ],
    "**/*.{css,}": [
      "npx stylelint --fix src/components/**/*.css"
    ]
  }
}
