{
  "name": "@trendyol-js/grace",
  "version": "1.0.15",
  "description": "Design System written with Vue.js",
  "license": "MIT",
  "repository": "https://github.com/Trendyol/grace",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run scss && node --max_old_space_size=7000 node_modules/rollup/dist/bin/rollup -c rollup.config.ts --environment BUILD:production",
    "clean": "rimraf css && rimraf core && rimraf directives && rimraf plugins",
    "test:unit": "vue-cli-service test:unit",
    "snap:update": "vue-cli-service test:unit -u",
    "test:cov": "vue-cli-service test:unit --coverage --runInBand --logHeapUsage",
    "lint": "vue-cli-service lint",
    "loki:approve": "loki approve",
    "loki:update": "loki update",
    "loki:test": "loki test --chromeTolerance 1",
    "loki:build:test": "npm run storybook:build && loki --requireReference --reactUri file:./storybook-static",
    "build:watch": "rollup --watch -c --environment BUILD:production",
    "scss": "node-sass design-system/system.scss -o css/ && node-sass design-system/root.scss -o css/",
    "storybook:build": "npm run scss && build-storybook",
    "storybook:start": "npm run scss && start-storybook -s ./ -p 6006",
    "deploy-storybook": "storybook-to-ghpages -- --source-branch=main",
    "docs": "bash ./generateDocs.sh"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "dayjs": "^1.8.28",
    "feather-icons": "^4.28.0",
    "inputmask": "5.0.0",
    "vue": "2.6.11",
    "vue-quill-editor": "^3.0.6",
    "vue2-datepicker": "^3.8.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.11.0",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.3.3",
    "@rollup/plugin-typescript": "5.0.2",
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-backgrounds": "5.3.18",
    "@storybook/addon-centered": "5.3.18",
    "@storybook/addon-contexts": "5.3.18",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-notes": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/source-loader": "5.3.18",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/vue": "5.3.18",
    "@types/feather-icons": "4.7.0",
    "@types/jest": "23.3.14",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "@vuedoc/md": "^3.2.0",
    "@vuedoc/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "husky": "^3.0.9",
    "jest": "^25.4.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-transform-stub": "^2.0.0",
    "loki": "^0.20.3",
    "node-sass": "^4.14.1",
    "rimraf": "3.0.2",
    "rollup": "2.23.1",
    "rollup-plugin-scss": "2.5.0",
    "rollup-plugin-svg": "2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "sass-loader": "8.0.0",
    "storybook-design-token": "^0.7.2",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^7.0.4",
    "typescript": "~3.8.3",
    "vue-loader": "15.9.3",
    "vue-template-compiler": "2.6.11"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}
